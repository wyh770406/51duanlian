<% content_for :title do %>
<h1><%= t 'crud.listing', model: VenueType.model_name.human %></h1>
<div class="toolbar">
  <%= link_to t('crud.new'), new_admin_venue_type_path, class: 'btn btn-primary' %>
</div>
<% end %>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th><%= VenueType.human_attribute_name(:name) %></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
<% @venue_types.each do |venue_type| %>
    <tr>
      <td><%= venue_type.name %></td>
      <td class="control span4">
        <%= link_to t('crud.edit'), edit_admin_venue_type_path(venue_type) %>
        <%= link_to t('crud.destroy'), admin_venue_type_path(venue_type), confirm: t('crud.destroy_confirmation'), method: :delete %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>
